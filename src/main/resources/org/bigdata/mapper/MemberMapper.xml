<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
 <mapper namespace="org.bigdata.mapper.MemberMapper">
 	<insert id="memberJoin">
		insert into shopping_member values(#{memberId}, 
		#{memberPassword},#{memberName}, #{memberEmail},#{memberAddr}, #{memberAddr1}, 
		#{memberAddr2},#{memberAddr3}, 0, 100000, 5000,sysdate,sysdate)
	</insert>
	
	<!-- 아이디 중복검사 -->
  	<select id="idCheck" resultType="int">
  		select count(*) from shopping_member where memberId = #{memberId}
  	</select>

 </mapper>   